public class Main {
    public static void main(String[] args) {
        System.out.println("Números um abaixo do outro:");
        for (int i = 1; i <= 20; i++) {
            System.out.println(i);
        }
        System.out.println("\nNúmeros um ao lado do outro:");
        for (int i = 1; i <= 20; i++) {
            System.out.print(i + " ");
        }
    }
}
